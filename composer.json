{
    "name": "acquia/lightning-project",
    "description": "Project template for Drupal 8 sites built with the Lightning distribution.",
    "type": "project",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "drupal/drupal-extension": "~3.4.0",
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8",
        "behat/behat": "^3.0",
        "se/selenium-server-standalone": "^2.53",
        "composer/composer": "^1.3",
        "drupal/twig_xdebug": "^1.1"
    },
    "require": {
        "drupal-composer/drupal-scaffold": "^2.0.0",
        "cweagans/composer-patches": "^1.6.0",
        "acquia/lightning": "~4.0.4",
        "drupal/bootstrap_clean_blog": "^1.0@RC",
        "drupal/bootstrap_paragraphs": "2.x",
        "drupal/devel": "^2.0",
        "drupal/adminimal_admin_toolbar": "^1.5",
        "drupal/twig_tweak": "^1.9",
        "drupal/module_filter": "^3.1",
        "drupal/bootstrap": "^3.7",
        "drupal/google_analytics": "^2.2",
        "drupal/adsense": "^1.0",
        "drush/drush": "^9.0",
        "drupal/ckeditor_markdown": "^1.0",
        "drupal/markdown": "^1.2",
        "drupal/disqus": "^1.0@RC",
        "drupal/sharethis": "^2.0@beta",
        "drupal/codesnippet": "^1.6",
        "drupal/xmlsitemap": "^1.0@alpha",
        "drupal/simple_sitemap": "^3.0",
        "drupal/codetag": "^1.1",
        "drupal/ctools": "^3.0",
        "drupal/tui_editor": "^1.9",
        "drupal/honeypot": "^1.29",
        "drupal/redirect": "^1.3",
        "drupal/config_split": "^1.4",
        "drupal/stage_file_proxy": "^1.0@beta",
        "drupal/seckit": "^1.1",
        "drupal/entity_browser": "^2.1",
        "drupal/consumers": "^1.9",
        "drupal/bootstrap4": "^1.0",
        "drupal/fontawesome": "^2.13",
        "drupal/optional_end_date": "^1.0",
        "drupal/cookieconsent": "^1.4",
        "drupal/consumer_image_styles": "^3.0@rc",
        "drupal/contentajs": "^1.0@beta",
        "drupal/admin_toolbar": "^1.21",
        "drupal/adminimal_theme": "^1.3",
        "drupal/build_hooks": "^2.1",
        "drupal/console": "^1.0.2",
        "drupal/core": "~8.7",
        "drupal/field_group": "^3.0",
        "drupal/gatsby": "^1.0@alpha",
        "drupal/decoupled_router": "^1.1",
        "drupal/image_widget_crop": "^2.1",
        "drupal/inline_entity_form": "^1.0@beta",
        "drupal/jsonapi_extras": "^3.6",
        "drupal/jsonrpc": "^1.0@beta",
        "drupal/material_admin": "^1.0@alpha",
        "drupal/openapi": "^1.0@beta",
        "drupal/schemata": "^1.0@alpha",
        "drupal/simple_oauth": "^3.3",
        "drupal/subrequests": "^2.0",
        "drupal/video_embed_field": "^2.0",
        "drupal/openapi_ui": "^1.0@rc",
        "drupal/openapi_ui_redoc": "^1.0@rc",
        "drupal/paragraphs": "^1.2",
        "drupal/pathauto": "^1.0",
        "drupal/site_settings": "^1.12",
        "drupal/subpathauto": "^1.0@RC",
        "drupal/token": "^1.1",
        "drupal/yaml_content": "^1.0",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "drupal/media_entity_instagram": "^2.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "scripts": {
      "post-install-cmd": [
        "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
      ],
      "post-update-cmd": [
        "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
      ]
    },
    "config": {
        "bin-dir": "bin/"
    },
    "extra": {
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "enable-patching": true,
        "patches": {
            "drupal/sharethis": {
              "Strings in brackets": "https://www.drupal.org/files/issues/Error-operator-not-supported-for-strings-2885710-12128018.patch"
            },
            "drupal/core": {
              "8.7 issue": "https://www.drupal.org/files/issues/2019-05-23/jsonapi_2996114.patch"
            },
            "drupal/tui_editor": {
              "select formats": "https://www.drupal.org/files/issues/2019-07-08/3045979-issue-with-selection.patch"
            },
            "drupal/cookieconsent": {
              "webprofiler issues": "https://www.drupal.org/files/issues/2018-07-12/cookieconsent-Use_EntityTypeManagerInterface_instead_of_EntityTypeManager-2985272-2.patch"
            }
        },
        "drupal-scaffold": {
          "excludes": [
            ".csslintrc",
            ".editorconfig",
            ".eslintignore",
            ".gitattributes",
            ".htaccess",
            "index.php",
            "robots.txt",
            "sites/default/default.settings.php",
            "sites/default/default.services.yml",
            "sites/development.services.yml",
            "sites/example.settings.local.php",
            "sites/example.sites.php"
          ]
        }
    }
}
